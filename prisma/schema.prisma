model igreja {
  id              Int       @id @default(autoincrement())
  codigo          String    @db.VarChar(20)
  nome            String    @db.Text
  foneSecretaria  String?   @db.VarChar(15)
  contatoWhatsApp String?   @db.VarChar(15)
