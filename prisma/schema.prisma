  id          Int      @id @default(autoincrement())
  idTurma     BigInt?  @default(0)
}

model Nucleos {
  idNucleo   BigInt  @id
  nomeNucleo String  @db.VarChar(255)
  Endereco   String? @db.VarChar(250)
}

model agenda_igreja {
  id_agenda BigInt   @id @default(autoincrement())
  Descricao String   @db.VarChar(255)
  Objetivo  String?  @db.Text
  DataIni   DateTime @db.DateTime
  DataFim   DateTime @db.DateTime
}

model TB_PLANO_CONTA {
  codigo     String   @id @db.VarChar(20)
  Descricao  String?  @db.VarChar(255)
  Reduz      String?  @default("0") @db.VarChar(20)
  analitica  Boolean?
  patrimonio Boolean?
  natureza   String?  @db.VarChar(100)
}

model TB_MES_REFERENCIA {
  mesReferencia        BigInt  @default(0)
  anoReferencia        BigInt  @default(0)
  mesAtivo             Boolean
  mesExtensoReferencia String  @db.VarChar(50)

  @@id([mesReferencia, anoReferencia])
}
