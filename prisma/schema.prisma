generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model avisos {
  Data     DateTime @id @db.DateTime
  avisos   String?  @db.VarChar(255)
  conteudo String?  @db.Text
  Distrito Int?     @default(0)
}

model boletim {
  id         Int       @id @default(autoincrement())
  Data       DateTime? @db.DateTime
  semana     Int       @default(0)
  titulo     String?   @db.Text
  introducao String?   @db.Text
  ponto1     String?   @db.VarChar(255)
  conteudo1  String?   @db.VarChar(4000)
  ponto2     String?   @db.VarChar(255)
  conteudo2  String?   @db.VarChar(500)
  ponto3     String?   @db.VarChar(255)
  conteudo3  String?   @db.VarChar(500)
  ponto4     String?   @db.VarChar(255)
  conteudo4  String?   @db.VarChar(500)
  ponto5     String?   @db.VarChar(255)
  conteudo5  String?   @db.VarChar(500)
  ponto6     String?   @db.VarChar(255)
  conteudo6  String?   @db.VarChar(500)
  ponto7     String?   @db.VarChar(255)
  conteudo7  String?   @db.VarChar(500)
  ponto8     String?   @db.VarChar(255)
  conteudo8  String?   @db.VarChar(500)
  ponto9     String?   @db.VarChar(255)
  conteudo9  String?   @db.VarChar(500)
  ponto10    String?   @db.VarChar(255)
  conteudo10 String?   @db.VarChar(500)
  conclusao  String?   @db.VarChar(4000)
  Autor      String    @db.VarChar(200)
}

model celulas {
  Celula        Int       @default(0)
  Nome          String    @db.VarChar(255)
  Supervisao    Int       @default(0)
  Anfitriao     String?   @db.VarChar(255)
  CriadoEm      DateTime? @db.DateTime
  CriandoPor    String?   @db.VarChar(145)
  AtualizadoEm  DateTime? @db.DateTime
  AtualizadoPor String?   @db.VarChar(255)
  Distrito      Int       @default(0)
  Coordenacao   Int       @default(0)

  @@id([Celula, Supervisao, Coordenacao, Distrito])
}

model contribuicoes {
  id        Int       @id @default(autoincrement())
  RolMembro Int       @default(0)
  Nome      String    @db.VarChar(255)
  Semana    Int?      @default(0)
  Mes       Int?      @default(0)
  Data      DateTime  @db.DateTime
  Ano       Int?      @default(0)
  Tipo      String    @db.VarChar(45)
  Valor     String    @db.VarChar(145)
  CriadoPor String?   @db.VarChar(150)
  CriandaEm DateTime? @db.DateTime
}

model cordenacao {
  Coordenacao      Int       @default(0)
  Coordenacao_Nome String    @db.VarChar(150)
  Distrito         Int       @default(0)
  CriadoEm         DateTime? @db.DateTime
  CriadoPor        String?   @db.VarChar(150)
  AtualizadoEm     DateTime? @db.DateTime
  AtualizadoPor    String?   @db.VarChar(255)

  @@id([Coordenacao, Distrito])
}

model cursos {
  codigoCurso  BigInt  @id @default(0)
  Curso        String  @db.VarChar(255)
  imagemCurso  String? @db.VarChar(255)
  PreRequisito Boolean @default(false)
  Requisito    BigInt? @default(0)
  inscricao    Boolean
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model dicas {
  id   Int    @id @default(autoincrement())
  Dica String @db.Text
}

model distrito {
  Distrito      Int       @id @default(0)
  Distrito_Nome String    @db.VarChar(200)
