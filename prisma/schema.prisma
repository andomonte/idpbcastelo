}

model inscritosCursos {
  id              Int       @unique @default(autoincrement())
  idTurma         BigInt    @default(0)
  codigoCurso     BigInt    @default(0)
  Curso           String    @db.VarChar(255)
  DataCurso       DateTime  @db.DateTime
  Documento       String    @default("0") @db.VarChar(50)
  RolMembro       Int       @default(0)
  Nome            String    @db.VarChar(255)
  Nascimento      DateTime? @db.Date
  Contato         String?   @db.VarChar(20)
  Email           String?   @db.VarChar(255)
  Igreja          String    @db.VarChar(20)
  Status          String?   @db.VarChar(50)
  Nota            String?   @db.VarChar(45)
  Frequencia      String?   @db.VarChar(45)
  Situacao        String?   @db.VarChar(50)
  CreatedAt       DateTime  @db.DateTime
  CriadoPor       String?   @db.VarChar(150)
  valor_pago      Decimal?  @default(0) @db.Decimal(10, 2)
  data_pagamento  DateTime? @db.Date
  forma_pagamento String?   @db.VarChar(50)
  participante    String?   @db.VarChar(50)

  @@id([codigoCurso, Documento, RolMembro])
}

model inscritosEventosGeral {
  id              Int       @unique @default(autoincrement())
  idEvento        Int       @default(0)
  Evento          String?   @db.Text
  Identificador   String    @db.VarChar(45)
  Nome            String    @db.VarChar(255)
